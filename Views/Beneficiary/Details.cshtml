@model AyushmanBharat.Models.ViewModels.BeneficiaryVM

@{
    ViewBag.Title = "Beneficiary";
 }

<section class="content-header">
    <h1>
        Policy Holder Details
    </h1>
</section>

<section class="content">
    <div class="row">
        <div class="col-md-12">


            <div class="form-horizontal">

                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Policy Holder Details<small class="text-muted">(Created On:@Model.CreatedOnDateString)</small></h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-info btn-sm" data-widget="collapse" data-toggle="tooltip" title="" data-original-title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                        <!-- /.box-tools -->
                    </div>
                    <div class="box-body pad">

                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryName)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryName)
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryGender)</strong>
                            </div>
                            <div class="col-md-8">
                                @if (Convert.ToString(Model.BeneficiaryGender) == "Female")
                                {
                                    <span>Female</span>
                                }
                                else if (Convert.ToString(Model.BeneficiaryGender) == "Male")
                                {
                                    <span>Male</span>
                                }
                                else
                                {
                                    <span>All</span>
                                }
                            </div>
                        </div>


                        <div class="clearfix"></div>

                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryAge)</strong>
                            </div>
                            <div class="col-md-8">
                                <div class="input-group">
                                    @Html.DisplayFor(model => model.BeneficiaryAge)

                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.InsuranceValidityDateString)</strong>
                            </div>
                            <div class="col-md-8">
                                <div class="input-group">
                                    @Html.DisplayFor(model => model.InsuranceValidityDateString)

                                </div>
                            </div>
                        </div>


                        <div class="clearfix"></div>


                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryRenewalCode)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryRenewalCode)
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryRenewalID)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryRenewalID)
                            </div>
                        </div>



                        <div class="clearfix"></div>

                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryCode)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryCode)
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryFamilyCode)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryFamilyCode)
                            </div>
                        </div>

                        <div class="clearfix"></div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.InsuranceCompanyName)</strong>
                            </div>
                            <div class="col-md-8">

                                @Html.DisplayFor(t => t.InsuranceCompanyName)

                            </div>
                        </div>


                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Policy Holder Contact Details</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-info btn-sm" data-widget="collapse" data-toggle="tooltip" title="" data-original-title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                        <!-- /.box-tools -->
                    </div>
                    <div class="box-body pad">
                        <div class="clearfix"></div>

                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryContactNo)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryContactNo)
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryLandlineNumber)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryLandlineNumber)
                            </div>
                        </div>

                        <div class="clearfix"></div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.StateName)</strong>
                            </div>
                            <div class="col-md-8">

                                @Html.DisplayFor(t => t.StateName)

                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.DistrictName)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(t => t.DistrictName)

                            </div>
                        </div>

                        <div class="clearfix"></div>

                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BlockName)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BlockName)
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.TalukaName)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.TalukaName)
                            </div>
                        </div>

                        <div class="clearfix"></div>

                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryLocality)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryLocality)
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryLandmark)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryLandmark)
                            </div>
                        </div>
                        <div class="clearfix"></div>

                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.PinCode)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.PinCode)
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <div class="col-md-4">
                                <strong>@Html.DisplayNameFor(model => model.BeneficiaryAddress)</strong>
                            </div>
                            <div class="col-md-8">
                                @Html.DisplayFor(model => model.BeneficiaryAddress)
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>


                <div class="form-group col-md-12">
                    <center>
                        <a href="@Url.Action("Edit", "Beneficiary", new { id = Model.BeneficiaryID})" title="Edit" class="btn btn-success">Edit</a>
                        <a class="btn btn-info" title="Back To List" href="@Url.Action("Index","Beneficiary" )"> Back To List</a>
                    </center>
                </div>
            </div>


        </div>
    </div>
